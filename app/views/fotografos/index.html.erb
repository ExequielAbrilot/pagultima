<% @fotografo.each do |f| %>
  <tr>
    <td><%= f.nombrePila %></td>
    <td><%= f.fechaNacimiento %></td>
    <td><%= link_to 'Show', fotografo_path(f) %></td>
    <td><%= link_to 'Edit', edit_fotografo_path(f) %></td>
    <td><%= link_to 'Destroy', fotografo_path(f),
                    method: :delete, data: { confirm: 'Esta seguro?' } %></td>
    </br>
  </tr>
<% end %>
</table>
